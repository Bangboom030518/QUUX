{
  "files.exclude": {
    "..gitignore": true,
    ".vscode": true,
    "**/*.lock": true,
    "target": true,
    ".devcontainer": true,
    "assets": true
  },
  "rust-analyzer.cargo.target": "wasm32-unknown-unknown",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.validate": false,
  "editor.inlineSuggest.enabled": true
}
