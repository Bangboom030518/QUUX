{
  "files.exclude": {
    "..gitignore": false,
    ".vscode": false,
    "**/*.lock": false,
    "target": false,
    ".devcontainer": false,
    "assets": false
  },
  "rust-analyzer.cargo.target": "wasm32-unknown-unknown",
  "files.associations": {
    "*.css": "postcss"
  },
  // "editor.lineNumbers": "on",
  "postcss.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.validate": false,
  "editor.inlineSuggest.enabled": true,
  "rust-analyzer.showUnlinkedFileNotification": false
}
